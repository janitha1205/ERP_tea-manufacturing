

@Model Dictionary<int,ERP_1.Models.Material>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/material_Layout.cshtml";
    }


    <h2>Index</h2>
    <table>
        <tr>
            <th>No</th>
            <th>Name</th>
            <th>Requirement</th>
            <th>Availability</th>
            <th>Note</th>
            <th>Staff ID</th>
            <th>Issued</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Key </td>
                <td>@item.Value.Name</td>
                <td>@item.Value.Required_ammount</td>
                <td>@item.Value.Available_ammount</td>
                <td>@item.Value.Note</td>
                <td>@item.Value.Staff_id</td>
                <td>@item.Value.IssueID</td>
            </tr>
        }
    </table>

