


@Model Dictionary<int,ERP_1.Models.Mechines>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/employee_Layout.cshtml";
    }


    <h2>Index</h2>
     <table>
        <tr>
            <th>No</th>
            <th>MechName</th>
            <th>Description</th>
            <th>Type</th>
            <th>LabourID</th>
            <th>IsDown</th>
            <th>HoursWorked</th>
         </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Key </td>
                <td>@item.Value.MechName</td>
                <td>@item.Value.Description</td>
                <td>@item.Value.Type</td>
                <td>@item.Value.LabourID</td>
                <td>@item.Value.IsDown</td>
                <td>@item.Value.HoursWorked</td>
            </tr>
        }
    </table>
