@Model Dictionary<int,ERP_1.Models.Employee>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/employee_Layout.cshtml";
    }


    <h2>Index</h2>
    <span id="Date"></span>
    <input id="GetDateWithJson" type="submit" value="Get Date" />
    <input type="submit" value="Goto Update Employee" onclick="window.location.href='UpdateEmployee'" />
    <br />
    <p>
        Name=@ViewData["Name"]
    </p>
    <br />
    <p>
        SureName=@ViewBag.SureName
    </p>
    <br />
    <a href="@Url.RouteUrl("Product", new {@Title="Product/index"})"><strong>Product</strong></a>
    <br />
    <a href="@Url.RouteUrl("Material", new {@Title="Material/index"})"><strong>Material</strong></a>
    <br />
    <a href="@Url.RouteUrl("Mechine", new {@Title="Mechine/index"})"><strong>Mechine</strong></a>
    <br />

    <a href="@Url.RouteUrl("WIP", new {@Title="WorkInProcess/index"})"><strong>Work in progress</strong></a>
    <br />
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>SureName</th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Value.SessionID </td>
                <td>@item.Value.Name</td>
                <td>@item.Value.SureName</td>
            </tr>
        }
    </table>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script>
        $(function() {
            $('#GetDateWithJson').click(
                function () {
                    $.ajax({
                        url: "@Url.Action("GetDateWithJson", "Employee")",
                        type: "POST",
                        success: function (result) {
                            $('#Date').html(result);
                        }

                    });


                }
            );



        });

    </script>
